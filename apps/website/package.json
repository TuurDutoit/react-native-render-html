{
  "name": "website",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus api:gen && DOCUSAURUS_NO_DUPLICATE_TITLE_WARNING=false docusaurus start --no-open",
    "build": "docusaurus api:gen && DOCUSAURUS_NO_DUPLICATE_TITLE_WARNING=false docusaurus build",
    "build-docs": "doc-mdx-gen-cli docs",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus api:gen && docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "test:ts": "tsc -p . --noEmit",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "api-gendoc": "docusaurus api:gen"
  },
  "dependencies": {
    "@doc/constants": "workspace:*",
    "@doc/svg-component": "workspace:*",
    "@docusaurus/core": "^2.0.0-beta.17",
    "@docusaurus/preset-classic": "^2.0.0-beta.17",
    "@mdx-js/react": "^2.0.0",
    "clsx": "^1.1.1",
    "docusaurus-plugin-sass": "^0.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modal": "^3.14.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@doc/mdx-gen-cli": "workspace:*",
    "@doc/pages": "workspace:*",
    "@doc/svgr-conf": "workspace:*",
    "@docusaurus/module-type-aliases": "^2.0.0-beta.17",
    "@tsconfig/docusaurus": "^1.0.4",
    "@types/react": "^17.0.39",
    "@types/react-helmet": "^6.1.5",
    "@types/react-modal": "^3.13.1",
    "@types/react-router-dom": "^5.3.3",
    "doc-docusaurus-rfg-plugin": "workspace:*",
    "doc-docusaurus-typedoc-plugin": "workspace:*",
    "sass": "^1.49.9",
    "typedoc": "^0.22.12",
    "typescript": "4.6.2"
  }
}
